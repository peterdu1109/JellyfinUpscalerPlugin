<?xml version="1.0" encoding="UTF-8"?>
<svg width="512" height="512" viewBox="0 0 512 512" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#3498db;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#2980b9;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1f4e79;stop-opacity:1" />
    </linearGradient>
    <filter id="glow">
      <feGaussianBlur stdDeviation="3" result="coloredBlur"/>
      <feMerge> 
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <!-- Background -->
  <rect width="512" height="512" rx="64" fill="url(#bgGradient)"/>
  
  <!-- Outer glow circle -->
  <circle cx="256" cy="256" r="200" fill="rgba(255,255,255,0.1)" filter="url(#glow)"/>
  
  <!-- AI Text -->
  <text x="256" y="220" font-family="Arial, sans-serif" font-size="80" font-weight="bold" 
        text-anchor="middle" dominant-baseline="middle" fill="white" filter="url(#glow)">AI</text>
  
  <!-- Upscale arrows -->
  <g stroke="#00d4ff" stroke-width="12" stroke-linecap="round" fill="none" filter="url(#glow)">
    <!-- Up arrow -->
    <path d="M 200 320 L 230 290 L 260 320"/>
    <!-- Right arrow  -->
    <path d="M 290 280 L 320 310 L 290 340"/>
  </g>
  
  <!-- Corner dots -->
  <g fill="#00d4ff" filter="url(#glow)">
    <circle cx="100" cy="100" r="8"/>
    <circle cx="412" cy="100" r="8"/>
    <circle cx="100" cy="412" r="8"/>
    <circle cx="412" cy="412" r="8"/>
  </g>
  
  <!-- Enhancement symbols -->
  <g fill="rgba(255,255,255,0.3)" font-family="Arial, sans-serif" font-size="24" text-anchor="middle">
    <text x="180" y="360">↗</text>
    <text x="332" y="360">✨</text>
  </g>
</svg>